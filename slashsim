getgenv().autoslice = false;
getgenv().autorebirth = false;

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Slashing Simulator", "Sentinel")
local Main = Window:NewTab("Main")
local MainSec = Main:NewSection("Main")
local Rebirths = Window:NewTab("Rebirths")
local RebirthSec = Rebirths:NewSection("Rebirths")
local Teleports = Window:NewTab("Teleports")
local TeleSec = Teleports:NewSection("World 1")
local TeleSec2 = Teleports:NewSection("World 2")
local TeleSec3 = Teleports:NewSection("World 3")

MainSec:NewToggle("Auto Slice", "Auto clicks", function(v)
    getgenv().autoslice = v
    spawn(function()
        while getgenv().autoslice == true do
            wait()
game:GetService("ReplicatedStorage").Remotes.Game.AirSlice:FireServer()

        end

end)
end)
RebirthSec:NewToggle("1 Rebirth", "1 Rebirth", function(v)
    getgenv().autorebirth = v
    spawn(function()
        while getgenv().autorebirth == true do
            wait()
local args = {
    [1] = 1,
    [2] = 1
}

game:GetService("ReplicatedStorage").Remotes.Other.Rebirth:FireServer(unpack(args))

        end
    end)
end)
