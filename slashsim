TeleSec:NewButton("Village", "Teleports to Village", function()
    function teleportTO(placeCFrame)
        local player = game.Players.LocalPlayer;
        if player.Character then
        player.Character.HumanoidRootPart.CFrame = placeCFrame;
        end
    end
    teleportTO(game:GetService("Workspace").WORLDS.World1.MapAssets.Village.EggStands.BasicEgg.Egg.CFrame)
    
    function teleportArea(zone)
        if game:GetService("Workspace").WORLDS.World1.MapAssets.Village.EggStands.BasicEgg.Egg:FindFirstChild(zone) then
            teleportTO(game:GetService("Workspace").WORLDS.World1.MapAssets.Village.EggStands.BasicEgg.Egg)
    end
    end
    
    teleportZone('Village')
end)
TeleSec:NewButton("Medieval Madness", "Teleports to Medieval Madness", function()
    function teleportTO(placeCFrame)
        local player = game.Players.LocalPlayer;
        if player.Character then
        player.Character.HumanoidRootPart.CFrame = placeCFrame;
        end
    end
    teleportTO(game:GetService("Workspace").WORLDS.World1.MapAssets["Medival Madness"].EggStands.WizardEgg.Egg.CFrame)
    
    function teleportArea(zone)
        if game:GetService("Workspace").WORLDS.World1.MapAssets["Medival Madness"].EggStands.WizardEgg.Egg:FindFirstChild(zone) then
            teleportTO(game:GetService("Workspace").WORLDS.World1.MapAssets["Medival Madness"].EggStands.WizardEgg.Egg)
    end
    end
    
    teleportZone('Medival Madness')
end)

